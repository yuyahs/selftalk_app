<% provide(:title, "回答一覧") %>

<button><%= link_to "マイページに戻る", current_user %></button>

<section class = "list">
  <% @questions.each do |question| %>
    <% @answers.each do |answer| %>
      <li> <%= question.content %> </li>
      <li> <%= answer.content %> </li>
      <button><%= link_to "添削する", edit_answer_path(answer) %></button>
    <% end %>
  <% end %>


</section>